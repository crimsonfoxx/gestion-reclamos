@model List<Projecto.Models.ViewModels.StudentsViewModel>
@{
    ViewBag.Title = "Quejas relacionadas a Profesores";
}

<h2>Quejas relacionadas a Profesores</h2>

<div class="col-md-12">
    <table class="table">
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Matricula</th>
            <th>Campus</th>
            <th>Queja</th>
            <th>Fecha</th>
            <th>Estatus</th>
            <th></th>
        </tr>
        @foreach (var oElemento in Model)
        {
        <tr>
            <td>@oElemento.Id</td>
            <td>@oElemento.Nombre</td>
            <td>@oElemento.Matricula</td>
            <td>@oElemento.Campus</td>
            <td>@oElemento.Queja</td>
            <td>@oElemento.Fecha.ToShortDateString()</td>
            <td>@oElemento.Estatus</td>
            <td>
                <a class="btn btn-default" href="~/Students/Editar/@oElemento.Id">Editar</a>
                <a class="btn btn-danger" href="~/Students/Eliminar/@oElemento.Id">Eliminar</a>

            </td>
        </tr>
        }
    </table>
</div>

